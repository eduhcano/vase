<h1><%= t("views.users.listing_users") %></h1>

<div id="users">
	<div id="left">
	<ul id="users">
	<% @users.each do |user| %>
		<li><%= link_to image_tag(user.profile.avatar.image.url(:thumb)), user_profile_path(user.login) %></li>
	<% end %>
	</ul>
	</div>
	<div id="right">
		<%= render :partial => 'shared/feed' %>
	</div>
</div>


